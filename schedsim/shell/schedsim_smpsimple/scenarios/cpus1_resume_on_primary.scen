#
#  Check suspending self, then resuming on primary CPU
#
rtems_init 1
task_create smp1 5
# GOAL: 0x0a010001 is heir and executing on CPU 0
cpus
# suspend running task
task_suspend smp1
# GOAL: 0x09010001 is heir and executing on CPU 0
cpus
# GOAL: 0x09010001 is heir and executing on CPU 0
cpus
# resume suspended task
task_resume smp1
# GOAL: 0x0a010001 is heir and executing on CPU 0
cpus
# GOAL: 0x0a010001 is heir and executing on CPU 0
cpus
